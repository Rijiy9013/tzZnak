#!/usr/bin/env php
<?php
declare(strict_types=1);

require __DIR__ . '/../vendor/autoload.php';

$container = require __DIR__ . '/../bootstrap/app.php';

$commands = require __DIR__ . '/../config/console.php';

$kernel = new \App\Console\ConsoleKernel($container, $commands);
$app = $kernel->create();
$app->run();
